print("Setup Turtle... Copying Files...")

local FileList = fs.list("disk/Turtle") --Table with all the files and directories available

-- copy the Turtle folder
for _, file in ipairs(FileList) do --Loop. Underscore because we don't use the key, ipairs so it's in order
  print("disk/Turtle/"..file) --Print the file name
  fs.copy("disk/Turtle/"..file, file)
end --End the loop

--fs.copy("disk/Turtle/startup", "startup")
--fs.copy("disk/Turtle/host", "host")

print("Setup Complete! Reboot.")